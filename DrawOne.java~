import edu.princeton.cs.algs4.StdDraw;

public class DrawOne{
  public static void main(String args){
    int N;
    StdDraw.setXscale(0, N);
    StdDraw.setYscale(0, N*N);
    StdDraw.setPenRadius(.01);
    for(int i = 0; i<= N; i++){
      StdDraw.point(i, 1);
      StdDraw.point(i, i*i);
    }
  }
}